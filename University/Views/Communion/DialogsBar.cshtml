@model IEnumerable<University.Models.Dto.DialogDto>

<div class="col-md-12" id="dialogsBar">
    <div class="btn-panel btn-panel-conversation" style="padding-left: 20px;">
        <a href="#" class="btn col-md-6 send-message-btn pull-left" style="background-color: #58d068; color: white;" onclick="NewDialog()">
            <i class="fa fa-plus"> Создать беседу </i>
        </a>
    </div>

    <div class="col-md-12" style="margin-top: 10px;">
        <div id="ListDialogs" class="media conversation" style="border: 3px;">
            @foreach (var dialog in Model)
            {
                @Html.Partial("PartialViewDialog", dialog)
            }
        </div>
    </div>
</div>
