@model University.Models.Dto.DialogDto

<tr class="dialog-container">
    <td>
        if(@Model.IsConversation)
        {
            <a href="#" onclick="OpenDialog(@Model.Id)">@Model.Name</a>
        }
        else
        {
            <a href="#" onclick="OpenDialog(@Model.Id)">@Model.Members[0].SurName @Model.Members[0].FirstName</a>
        }
    </td>
</tr>