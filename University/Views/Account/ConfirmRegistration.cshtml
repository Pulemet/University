@model IEnumerable<University.Models.ApplicationUser>

@{
    ViewBag.Title = "ConfirmRegistration";
}

@Scripts.Render("~/scripts/jquery-3.1.1.js")

<head>    
    <script type="text/javascript" src="/scripts/admin.js"> 
    </script>
</head>

<div class="container">
    <div class="col-lg-11 col-lg-offset-1">
        <h4 style="color: black;"> Ожидающие подтверждения регистрации </h4>
    </div>
    <div class="col-md-6 col-lg-offset-1" style="padding-top: 10px;">
        @foreach (var user in Model)
        {
            @Html.Partial("PartialAwaitingUser", user)
        }
    </div>
</div>

